<body class="{{body_class}}" data-template="base.twig">
  {% include 'partials/scripts-google-tag-manager.twig' %}
  {% include 'partials/svg-sprite.twig' %}
  <div class="o-offcanvas js-offcanvas" id="offcanvas-generationnyc">
    <a href="#main" class="show-for-keyboard">Skip to main content</a>
    <nav class="o-offcanvas__side js-offcanvas__side" id="generationnyc-menu" role="navigation" aria-label="generationnyc-menu-heading">
      <h2 id="generationnyc-menu-heading" class="show-for-screen-reader">Resources by Type</h2>
      <button data-toggle="offcanvas-generationnyc" class="o-offcanvas__close button--text-microsite"><svg class="icon--small" role="img"><title>Close Resources by Type</title><use xlink:href="#icon-close"></use></svg></button>
      <ol class="c-list-box c-list-box--microsite" data-multiselectable="true">
        {% for menu_item in ms_menu.get_items %}
          <li class="c-list-box__item">
            <h3 class="c-list-box__heading" id="generationnyc-menu-heading-{{loop.index}}" tabindex="0">
              <a href="{{menu_item.link}}" class="c-list-box__subitem">{{menu_item.title}}</a>
            </h3>
          </li>
        {% endfor %}
      </ol>
    </nav>

    <div class="o-offcanvas__main">
      {% block header %}
        <header class="c-banner">
          <div class="o-container">
            <a href="/generationnyc" class="c-banner__logo-wrapper"><span class="c-banner__logo c-banner-ms__logo">{{site.name}}</span></a>
            <nav class="c-banner__nav" role="navigation" aria-label="Primary">
              <ul class="o-inline o-inline-ms">
                {% for menu_item in ms_menu.get_items %}
                  <li class="o-inline__item desktop-only"><a href="{{menu_item.link}}" class="c-banner__link">{{menu_item.title}}</a></li>
                {% endfor %}
              {# Menu toggle on mobile #}
                <li class="o-inline__item mobile-only__nav"><a href="#mobile-nav" data-toggle="offcanvas-generationnyc" class="c-banner__link">Menu</a></li>
              {# end of Menu toggle on mobile #}

              {% if function('Search\\visible') %}
                <li class="o-inline__item desktop-only">
                   <a class="c-banner__link" href="#search" data-toggle="overlay-search" data-toggle-class="fadeInDown">
                     <svg class="c-banner__icon c-banner__icon__search" role="img"><title>Search</title><use xlink:href="#icon-search"></use></svg>
                     <svg class="c-banner__icon c-banner__icon__close" role="img"><title>Search</title><use xlink:href="#icon-close"></use></svg>
                   </a>
                </li>
              {% endif %}
              </ul>
            </nav>
          </div>
        </header>

        {# search #}
        {% if function('Search\\visible') %}
        <section class="o-overlay js-overlay animated" id="overlay-search" aria-hidden="true">
          <div class="o-container-med">

            <div class="u-bottom-spacing">
              {% include 'partials/search-form.twig' %}
            </div>

            <section class="o-col__divider c-block-list">
              <h2 class="c-block-list__heading">
                {{ __('Programs', 'guny') }}
                <a class="c-block-list__link" href="{{ site.url }}/guny/programs">{{ __('See all programs', 'guny') }}<svg role="img" class="icon--caret--xxsmall" aria-hidden="true"><use xlink:href="#icon-caret-right"></use></svg></a>
              </h2> <!-- /.c-content-tout__heading -->
              <div class="c-block-list__inner o-row">
                {{ top_programs }}
              </div> <!-- /.o-col__divider -->
            </section> <!-- /.c-content-tout -->

            <section class="o-col__divider c-block-list">
              <h2 class="c-block-list__heading">
                {{ __('Events', 'guny') }}
                <a class="c-block-list__link" href="{{ site.url }}/guny/events">{{ __('See all events', 'guny') }}<svg role="img" class="icon--caret--xxsmall" aria-hidden="small"><use xlink:href="#icon-caret-right"></use></svg></a>
              </h2> <!-- /.c-content-tout__heading -->
              <div class="c-block-list__inner o-row">
              {% for post in top_events %}
                <div class="o-col-3 c-content-tout">
                  <h3 class="c-content-tout__title">
                    <a href="{{ post.path }}" title="{{ post.post_title }}" rel="bookmark">{{ post.post_title }}</a>
                  </h3>
                  <div class="c-content-tout__subtitle">
                    {% include 'partials/event-date.twig' %}
                  </div> <!-- /.c-content-tout__subtitle -->
                  {% if post.venue %}
                    <div class="c-content-tout__link">
                      {% include 'partials/event-link.twig' %}
                    </div>
                  {% endif %}
                </div> <!-- /.o-col-3.c-content-tout -->
              {% endfor %}
              </div> <!-- /.o-col__row -->
            </section> <!-- /.o-col__divider -->
          </div> <!-- /.o-container-med -->
        </section> <!-- /.o-overlay -->
        {% endif %}
        {# search end #}
        
      {% endblock %}

      <main id="main" role="main">
        {% block content %}
          <section class="post-list small-12 columns">
            <p>Sorry, no entries match your criteria</p>
          </section> <!-- /.post-list.small-12.columns -->
        {% endblock %}
      </main> <!-- /#main -->

      <footer class="c-footer" role="contentinfo">
        <div class="o-container">
          <div class="c-footer__section">
            <div class="c-footer__logos">
              <figure class="c-footer__logo">
                <img alt="Generation NYC" src="{{theme.link}}/assets/img/logo-gnyc-white@236px.png" width="118">
              </figure>
              <figure class="c-footer__logo">
                <a href="http://www1.nyc.gov/site/childrenscabinet/index.page" target="_blank"><img alt="Link to NYC Children's Cabinet website" src="{{theme.link}}/assets/img/NYC-childrens-cabinet.svg" height="69" width="157"></a>
              </figure>
            </div>
            <nav role="navigation" class="c-footer__nav" aria-label="Secondary">
              <ul class="c-footer__column">
                <li class="c-footer__nav-item">
                  <a class="c-footer__link" href="{{ site.url }}/generationnyc/">{{ __('Home', 'GUNY-Footer') }}</a>
                </li>
                {% if ms_menu.id %}{% for m in ms_menu.get_items %}
                <li class="c-footer__nav-item {{ m._menu_item_classes|join(' ') }}">
                  <a class="c-footer__link" href="{{ m.link }}"
                  {% if m.attr_title != '' %}title="{{ m.attr_title }}"{% else %}title="{{ m.title }}"{% endif %}
                  {% if m.target != '' %}target="{{ m.target }}"{% endif %}
                  {% if m.xfn != '' %}rel="{{ m.xfn }}"{% endif %}
                  >
                    {{ m.title }}
                  </a>
                </li>
                {% endfor %}{% endif %}
                {% if ms_footer_menu_primary_additional.id %}{% for m in ms_footer_menu_primary_additional.get_items %}
                <li class="c-footer__nav-item {{ m._menu_item_classes|join(' ') }}">
                  <a class="c-footer__link" href="{{ m.link }}"
                  {% if m.attr_title != '' %}title="{{ m.attr_title }}"{% else %}title="{{ m.title }}"{% endif %}
                  {% if m.target != '' %}target="{{ m.target }}"{% endif %}
                  {% if m.xfn != '' %}rel="{{ m.xfn }}"{% endif %}
                  >
                    {{ m.title }}
                  </a>
                </li>
                {% endfor %}{% endif %}
                {% if footer_menu_left.id %}{% for m in footer_menu_left.get_items %}
                <li class="c-footer__nav-item {{ m._menu_item_classes|join(' ') }}">
                  <a class="c-footer__link" href="{{ m.link }}"
                  {% if m.attr_title != '' %}title="{{ m.attr_title }}"{% else %}title="{{ m.title }}"{% endif %}
                  {% if m.target != '' %}target="{{ m.target }}"{% endif %}
                  {% if m.xfn != '' %}rel="{{ m.xfn }}"{% endif %}
                  >
                    {{ m.title }}
                  </a>
                </li>
                {% endfor %}{% endif %}
              </ul>
              <ul class="c-footer__column">
                {% if ms_footer_menu_right.id %}{% for m in ms_footer_menu_right.get_items %}
                <li class="c-footer__nav-item {{ m._menu_item_classes|join(' ') }}">
                  <a class="c-footer__link" href="{{ m.link }}"
                  {% if m.attr_title != '' %}title="{{ m.attr_title }}"{% else %}title="{{ m.title }}"{% endif %}
                  {% if m.target != '' %}target="{{ m.target }}"{% endif %}
                  {% if m.xfn != '' %}rel="{{ m.xfn }}"{% endif %}
                  >
                    {{ m.title }}
                  </a>
                </li>
                {% endfor %}{% endif %}
              </ul>
            </nav>
          </div>
        </div> <!-- /.o-container -->
        <div class="o-container">
          <div class="c-footer__footer">
            <p>&copy; {{now|date('Y')}} {{ __('City of New York. All Rights Reserved.', 'GUNY-Footer') }}<br>{{ __('NYC is a trademark and service mark of the City of New York', 'GUNY-Footer') }}</p>
          </div>
        </div>
        <div class="c-footer__reached"></div>
      </footer> <!-- /.c-footer -->
      {% block banner_topic %}{% endblock %}
      {% if options.show_banner_generation and not disable_generation_banner %}
        <div class="c-alert js-alert" data-cookie="{{options.current_banner_generation.slug}}" id="banner-{{options.current_banner_generation.slug}}" role="dialog" aria-labelledby="dialog-title-{{options.current_banner.slug}}" aria-describedby="dialog-desc-{{options.current_banner.slug}}">
          <div class="show-for-screen-reader" id="dialog-title-{{options.current_banner.slug}}">Notification Banner</div>
          <div class="c-alert__banner" id="dialog-desc-{{options.current_banner.slug}}">
            <div class="o-container">
              {{options.current_banner_generation.get_field('banner_content')}}
            </div>
          </div>
          <button type="button" class="c-alert__button" data-toggle="banner-{{options.current_banner_generation.slug}}" aria-label="close"><svg role="img" class="icon--small"><title>Close</title><use xlink:href="#icon-close"></use></svg></button>
        </div>
      {% endif %}
      <a class="o-offcanvas__overlay" data-toggle="offcanvas-ages" aria-hidden="true"></a>
    </div> <!-- /.o-offcanvas__main -->
  </div> <!-- /.o-offcanvas -->

  {{ function('Nyco\\Enqueue\\script', 'source', '.min') }}
  {{ wp_footer }}

  <script type="text/javascript">
    var prod_dcsId_val = "dcs222bzsl0e9yqub15qsk92g_5z5q";
    var prod_host = "growingupnyc.cityofnewyork.us/generationnyc";
  </script>
  <script type="text/javascript" src="//www1.nyc.gov/assets/home/js/webtrends/webtrends_v10.js"></script>
</body>
