msgstr "Indeksointi"

#: lib/interface.php:1218
msgid "Attachments"
msgstr "Liitteet"

#: lib/interface.php:1219 lib/interface.php:1266 lib/interface.php:2658
msgid "Searching"
msgstr "Hakeminen"

#: lib/interface.php:1220
msgid "Logging"
msgstr "Lokit"

#: lib/interface.php:1221 lib/interface.php:1270
msgid "Excerpts and highlights"
msgstr "Otteet ja korostukset"

#: lib/interface.php:1222 lib/interface.php:2506 lib/interface.php:2511
msgid "Synonyms"
msgstr "Synonyymit"

#: lib/interface.php:1223 lib/interface.php:2537
msgid "Stopwords"
msgstr "Hukkasanat"

#: lib/interface.php:1225
