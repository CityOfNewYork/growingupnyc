<!DOCTYPE html>
<html lang="{{language_code}}" dir="{{direction}}" class="no-js">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="format-detection" content="telephone=no">
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="description" content="{{meta_desc}}">
  <meta name="keywords" content="{{meta_keywords}}">
  {% if meta_noindex == true %}
  <meta name="robots" content="noindex">
  {% endif %}
  <link rel="manifest" href="{{theme.path}}/manifest.json">
  <link rel="icon" type="image" href="{{theme.path}}/assets/img/growningup-favicon.png">
  <link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{theme.path}}/assets/img/growningup-Icon-App-76x76@3x.png">
  <link rel="stylesheet" href="{{theme.path}}/src/css/animate.css" type="text/css" media="screen">

  {{ function('enqueue_language_style', 'style') }}
  {{ function('wp_head') }}

  {% block head_extras %}{% endblock %}

  {% block scripts_head %}
    {% include 'services/google-analytics.twig' %}
    {% include 'services/google-tag-manager.twig' %}
  {% endblock %}

</head>

<body data-template="base.twig">
  {% include 'services/google-tag-manager-noscript.twig' %}

  <div class="o-offcanvas js-offcanvas" id="offcanvas-ages">

    <a href="#main" class="show-for-keyboard">{{__('Skip to main content', 'GUNYC')}}</a>

    {% include 'objects/navigation.twig' %}

    <div class="o-offcanvas__main js-offcanvas__main">

      {% include 'objects/navigation-offcanvas.twig' %}

      {# Main Content #}
      <main id="main" role="main">

        {% block content %}
          <section class="post-list small-12 columns">
            <p>Sorry, no entries match your criteria</p>
          </section>
        {% endblock %}

      </main> <!-- /#main -->

      {# Footer #}
      {% include 'objects/footer.twig' %}

      {# Alert Banner #}
      {% include 'objects/banner.twig' %}

    </div> <!-- /.o-offcanvas__main -->
  </div> <!-- /.o-offcanvas -->

  {{ function('enqueue_script', 'source') }}

  {{ wp_footer }}

  {# Icons #}
  <div class="hidden" aria-hidden="true">
    {% include 'partials/svg-sprite.twig' %}
  </div>

  {% block scripts_extras %}{% endblock %}

  </body>
</html>
